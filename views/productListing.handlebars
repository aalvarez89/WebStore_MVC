<article class="cards">

    {{#each data}}
    <div class="card">
        <span class="item">{{this.title}}</span>
        <a href="/user/productListing/{{this._id}}"><img class = "img" src=/img/{{this.imgUrl}} alt={{this.title}}></a>
        <span class="included">{{this.included}}</span>
        <span class ="synopsis">{{this.synopsis}}</span>
        <span class ="category">{{this.category}} Meal</span>
        {{!-- <p>{{this._id}}</p> --}}
        <a href="/user/productListing/{{this._id}}"><button class="buyButton">View Meal</button></a>
    </div>
    {{/each}}

</article>